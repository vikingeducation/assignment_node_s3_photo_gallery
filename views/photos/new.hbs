<h3 class="text-center">New Photo</h3>
<section class="well col-sm-4 col-sm-offset-4">
  <form
    action="/photos"
    method="post"
    enctype="multipart/form-data">

    <div class="form-group">
      <label for="photo_file">Photo File</label>
      <input type="file" class="form-control" name="photo[file]" id="photo_file">
    </div>

    <div class="form-group">
      <input type="submit" class="btn btn-primary">
    </div>
  </form>
</section>
