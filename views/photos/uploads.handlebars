<header class="page-header">
  <h1>New Photo</h1>
  <a href="/photos">Photos Index</a>
</header>

<form
  action="/photos/uploads"
  method="post"
  enctype="multipart/form-data">

  <div class="form-group">
    <label for="photo_file">Photo File</label>
    <input type="file" class="form-control" name="photo[file]" id="photo_file">
  </div>
<!--
  <div class="form-group">
    <label for="email_option_subject">User Id</label>
    <input type="hidden" id="email_option_subject" name="email_options[subject]" class="form-control" >
  </div> -->

  <div class="form-group">
    <label for="photo_description">Description</label>
    <input type="text" id="photo_description" name="photo[description]" class="form-control" >
  </div>

  <div class="form-group">
    <input type="submit" class="btn btn-primary">
  </div>
</form>
