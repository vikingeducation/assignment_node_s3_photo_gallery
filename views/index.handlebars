<h1>photo uploader</h1>

<form
  action="/photos/new"
  method="post"
  enctype="multipart/form-data">

  <div class="form-group">
    <label for="photo">Photo File</label>
    <input type="file" class="form-control" name="photo" id="photo">
  </div>

  <div class="form-group">
    <input type="submit" class="btn btn-primary">
  </div>
</form>
