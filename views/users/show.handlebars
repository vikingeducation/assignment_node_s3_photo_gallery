<div class="row">
  <div class="col-xs-8 col-offset-2"  >

    <p>Name: {{ selectedUser.username }}</p>

  </div>
</div>

<div class="row">
<h2>Photos: </h2>
{{#each photos as |photo key|}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <a href="/photos/{{photo.key}}"><img src="{{ photo.url }}" alt="Photo"></a>
      <div class="caption">
        <h3>{{ photo.photoName }}</h3>
      </div>
    </div>
  </div>
  {{/each}}
</div>